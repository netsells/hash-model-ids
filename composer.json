{
    "name": "netsells/hash-model-ids",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Moore",
            "email": "tom.moore@netsells.co.uk"
        }
    ],
    "require": {
        "php": "^8.1",
        "hashids/hashids": "^4.1",
        "laravel/framework": "^9.1"
    },
    "require-dev": {
        "orchestra/testbench": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Netsells\\HashModelIds\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Netsells\\HashModelIds\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Netsells\\HashModelIds\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
