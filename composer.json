{
    "name": "netsells/hash-model-ids",
    "description": "Dynamically create a hash of an Eloquent model id value to avoid exposing a record's actual database id.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "netsells",
        "eloquent",
        "ids",
        "hashids",
        "dynamic"
    ],
    "authors": [
        {
            "name": "Tom Moore",
            "email": "tom.moore@netsells.co.uk"
        },
        {
            "name": "Sam Jordan",
            "email": "sam.jordan@netsells.co.uk"
        }
    ],
    "require": {
        "php": "^8.1",
        "hashids/hashids": "^4.1",
        "laravel/framework": "^9.1"
    },
    "require-dev": {
        "orchestra/testbench": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Netsells\\HashModelIds\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Netsells\\HashModelIds\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Netsells\\HashModelIds\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
